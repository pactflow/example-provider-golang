{
  "consumer": {
    "name": "pactflow-example-consumer-golang"
  },
  "interactions": [
    {
      "description": "a request to get a product",
      "providerState": "a product with ID 10 exists",
      "request": {
        "method": "GET",
        "path": "/product/10"
      },
      "response": {
        "body": {
          "id": "10",
          "name": "pizza",
          "type": "food"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body.id": {
            "match": "type"
          },
          "$.body.name": {
            "match": "type"
          },
          "$.body.type": {
            "match": "type"
          },
          "$.headers['Content-Type']": {
            "match": "regex",
            "regex": "application/json;?.*"
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pactRust": {
      "ffi": "0.4.26",
      "mockserver": "1.2.11",
      "models": "1.2.7"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "pactflow-example-provider-golang"
  }
}